{
  "name": "phantom-frontend-elites",
  "version": "1.0.0",
  "description": "Phantom Project",
  "main": "index.js",
  "scripts": {
    "start": "serve -s ./dist",
    "build": "webpack --config webpack.config.js --mode production",
    "dev": "webpack-dev-server --mode development --open",
    "prepare": "husky install",
    "test": "jest",
    "test:watch": "npm run test -- --watch",
    "coverage": "jest --coverage --colors"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/atlp-rwanda/Phantom-Frontend-Elites.git"
  },
  "keywords": [],
  "author": "Code Elites",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/atlp-rwanda/Phantom-Frontend-Elites/issues"
  },
  "homepage": "https://github.com/atlp-rwanda/Phantom-Frontend-Elites#readme",
  "devDependencies": {
    "@babel/core": "^7.17.8",
    "@babel/plugin-proposal-class-properties": "^7.16.7",
    "@babel/plugin-transform-runtime": "^7.17.0",
    "@babel/preset-env": "^7.16.11",
    "@babel/preset-react": "^7.16.7",
    "@testing-library/react": "^12.1.2",
    "@types/enzyme": "^3.10.12",
    "@types/jest": "^27.4.1",
    "@wojtekmaj/enzyme-adapter-react-17": "^0.6.7",
    "babel-jest": "^27.5.1",
    "babel-loader": "^8.2.4",
    "css-loader": "^6.7.1",
    "enzyme": "^3.11.0",
    "eslint": "^7.32.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-config-standard": "^16.0.3",
    "eslint-plugin-import": "^2.25.4",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-promise": "^5.2.0",
    "eslint-plugin-react": "^7.29.4",
    "file-loader": "^6.2.0",
    "html-webpack-plugin": "^5.5.0",
    "husky": "^7.0.4",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^27.5.1",
    "jest-svg-transformer": "^1.0.0",
    "lint-staged": "^12.3.7",
    "mini-css-extract-plugin": "^2.6.0",
    "node-sass": "^7.0.1",
    "prettier": "^2.6.1",
    "tailwindcss": "^3.0.23",
    "webpack": "^5.71.0",
    "webpack-cli": "^4.9.2",
    "webpack-dev-server": "^4.7.4",
    "webpack-pwa-manifest": "^4.3.0"
  },
  "dependencies": {
    "@fortawesome/fontawesome-svg-core": "^6.1.1",
    "@material-ui/core": "4.12.1",
    "@material-ui/lab": "4.0.0-alpha.60",
    "axios": "^0.26.1",
    "@fortawesome/free-brands-svg-icons": "^6.1.1",
    "@fortawesome/free-solid-svg-icons": "^6.1.1",
    "@fortawesome/react-fontawesome": "^0.1.18",
    "autoprefixer": "^10.4.5",
    "css-loader": "^6.7.1",
    "dotenv": "^16.0.0",
    "font-awesome": "^4.7.0",
    "leaflet-routing-machine": "3.2.12",
    "leaflet": "^1.7.1",
    "leaflet-geosearch": "^3.6.0",
    "node": "^16.14.0",
    "postcss-loader": "^6.2.1",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-google-login": "^5.2.2",
    "react-phone-number-input": "^3.1.52",
    "react-loader-spinner": "^6.0.0-0",
    "react-redux": "^7.2.8",
    "react-router-dom": "^6.3.0",
    "react-spinners": "^0.11.0",
    "react-toastify": "^8.2.0",
    "react-validation": "^3.0.7",
    "react-leaflet": "^3.2.5",
    "redux": "^4.1.2",
    "redux-thunk": "^2.4.1",
    "serve": "^13.0.2"
  },
  "lint-staged": {
    "*.{js, jsx}": "eslint --cache --fix",
    "*.{js,jsx,css,md}": "prettier --write"
  },
  "engines": {
    "node": "16.14.0"
  },
  "transform": {
    "\\.svg$": "jest-svg-transformer"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "jsx"
    ],
    "modulePaths": [
      "src/components",
      "src/tests"
    ],
    "setupFilesAfterEnv": [
      "<rootDir>/enzyme.config.js"
    ],
    "collectCoverageFrom": [
      "<rootDir>/src/components/**/*.{js,jsx,ts,tsx}",
      "<rootDir>/src/redux/**/*.{js,jsx,ts,tsx}",
      "<rootDir>/src/skeleton/**/*.{js,jsx,ts,tsx}"
    ],
    "testEnvironment": "jsdom",
    "collectCoverage": true,
    "coverageThreshold": {
      "global": {
        "branches": 80,
        "functions": 80,
        "lines": 80,
        "statements": 80
      }
    },
    "moduleNameMapper": {
      "\\.(jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": "<rootDir>/__mocks__/fileMock.js",
      "\\.(css|less|scss)$": "identity-obj-proxy"
    }
  }
}
